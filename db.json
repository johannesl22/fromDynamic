[
   {
      "fieldGroupClassName":"row",
      "fieldGroup":[
         {
            "className":"col-6",
            "type":"input",
            "key":"firstName",
            "templateOptions":{
               "label":"First Name",
               "required":true,
               "placeholder":"Example:Jose"
            }
         },
         {
            "className":"col-6",
            "type":"input",
            "key":"lastName",
            "templateOptions":{
               "label":"Last Name",
               "required":true,
               "placeholder":"Example:Rodriguez"
            }
         }
      ]
   },
   {
      "className":"section-label",
      "template":"<hr /><div><strong>Address:</strong></div>"
   },
   {
      "fieldGroupClassName":"row",
      "fieldGroup":[
         {
            "className":"col-6",
            "type":"input",
            "key":"street",
            "templateOptions":{
               "label":"Street",
               "placeholder":"AV o Calle",
               "required":true
            }
         },
         {
            "className":"col-3",
            "type":"input",
            "key":"cityName",
            "templateOptions":{
               "label":"City",
               "placeholder":"Madrid",
               "required":true
            }
         },
         {
            "className":"col-3",
            "type":"input",
            "key":"zip",
            "templateOptions":{
               "type":"number",
               "label":"Zip",
               "max":99999,
               "min":0,
               "pattern":"\\d{5}"
            }
         }
      ]
   },
   {
      "className":"section-label",
      "template":"<hr /><div><strong>Contanto:</strong></div>"
   },
   {
      "fieldGroupClassName":"row",
      "fieldGroup":[
         {
            "className":"col-6",
            "type":"input",
            "key":"Email",
            "templateOptions":{
               "label":"Email",
               "required":true,
               "placeholder":"Alguien@algo.com",
                  "pattern":"/^([\w\.\-_]+)?\w+@[\w-_]+(\.\w+){1,}$/"
            }
         },
         {
            "className":"col-3",
            "type":"input",
            "key":"Movil",
            "templateOptions":{
               "label":"Movil",
               "required":true,
               "placeholder":"+3412365478"
            }
         },
         {
            "className":"col-3",
            "type":"input",
            "key":"Fijo",
            "templateOptions":{
               "label":"Fijo",
               "required":true,
               "placeholder":"3412365478"
            }
         }
      ]
   }
]
